{
  "source": {
    "include": "./src",
    "includePattern": "\\.js$",
    "excludePattern": "(node_modules/|docs)"
  },
  "opts": {
    "template": "node_modules/docdash/",
    "destination": "docs/",
    "recurse": true
  },

  "docdash": {
    "static": true,
    "sort": true,
    "search": true,
    "sectionOrder": ["Modules"],
    "menu": {
      "Github repo": {
        "href": "https://github.com/DwarfTelescopeUsers/dwarfii_api",
        "target": "_blank",
        "class": "menu-item",
        "id": "website_link"
      }
    }
  }
}
