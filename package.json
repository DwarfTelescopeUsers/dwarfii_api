{
  "name": "dwarfii_api",
  "version": "0.0.2-alpha.10",
  "description": "wrapper for Dwarf II telescope API",
  "main": "dist/index.js",
  "scripts": {
    "lint": "eslint src/**/*",
    "format": "prettier --check \"src/**/*.{ts,js}\"",
    "format:fix": "prettier --write \"src/**/*.{ts,js}\"",
    "CI": "npm run lint && npm run format && npm run typecheck",
    "generate-docs": "jsdoc -c jsdoc.json  -R README.md",
    "ts": "tsc -p ./tsconfig.json",
    "typecheck": "tsc -p ./tsconfig.json --noEmit",
    "build": "npm run CI && npm run ts && npm run generate-docs"
  },
  "type": "module",
  "author": "stevejcl",
  "repository": {
    "type": "git",
    "url": "https://github.com/stevejcl/dwarfii_api"
  },
  "license": "ISC",
  "devDependencies": {
    "docdash": "^2.0.1",
    "eslint": "^8.40.0",
    "eslint-config-prettier": "^8.8.0",
    "jsdoc": "^4.0.2",
    "prettier": "^2.8.8",
    "typescript": "^5.1.6"
  },
  "dependencies": {
    "intl": "^1.2.5"
  }
}
